{
  "name": "notes-app-aws-backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "test:unit:database-service": "nx test @notes-app/database-service --configuration=unit",
    "test:integration:database-service": "./scripts/test/integration/database-service.sh",
    "test:unit:queue-service": "nx test @notes-app/queue-service --configuration=unit",
    "test:integration:queue-service": "./scripts/test/integration/queue-service.sh",
    "test:unit:storage-service": "nx test @notes-app/storage-service --configuration=unit",
    "test:integration:storage-service": "./scripts/test/integration/storage-service.sh",
    "exe:database": "./scripts/dev/database-service.sh",
    "dev:auth-service": "nx build auth-service && node --env-file=.env-DEV dist/microservices/auth-service/main.js",
    "dev:note-service": "nx build note-service && node --env-file=.env-DEV dist/microservices/note-service/development/main.js",
    "dev:queue-service": "nx build queue-service && node --env-file=.env-DEV dist/microservices/queue-service/main.js",
    "build:auth-service": "nx run auth-service:build:production --skip-nx-cache",
    "build:note-service": "nx run note-service:build:production --skip-nx-cache",
    "build:queue-service": "nx run queue-service:build:production --skip-nx-cache",
    "build:playground": "nx run note-service:build:playground --skip-nx-cache",
    "publish": "./scripts/prod/publish.sh",
    "publish:auth-service": "./scripts/prod/publish.sh auth-service",
    "publish:playground": "./scripts/prod/publish.sh playground",
    "deploy": "./scripts/prod/deploy.sh .env-PROD.json",
    "deploy:auth-service": "./scripts/prod/deploy.sh .env-PROD.json auth-service",
    "deploy:note-service": "./scripts/prod/deploy.sh .env-PROD.json note-service",
    "deploy:playground": "./scripts/prod/deploy.sh .env-PROD.json playground"
    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rahulstech/notes-app-aws-backend.git"
  },
  "author": "Rahul Bagchi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rahulstech/notes-app-aws-backend/issues"
  },
  "homepage": "https://github.com/rahulstech/notes-app-aws-backend#readme",
  "description": "",
  "dependencies": {
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "pino": "^9.9.4",
    "serverless-http": "^4.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.913.0",
    "@aws-sdk/client-dynamodb": "^3.913.0",
    "@aws-sdk/client-s3": "^3.913.0",
    "@aws-sdk/client-sqs": "^3.913.0",
    "@aws-sdk/s3-request-presigner": "^3.913.0",
    "@aws-sdk/util-dynamodb": "^3.913.0",
    "@eslint/js": "^9.8.0",
    "@nx/esbuild": "^21.4.0",
    "@nx/eslint": "21.4.0",
    "@nx/eslint-plugin": "21.4.0",
    "@nx/jest": "21.4.0",
    "@nx/js": "^21.4.0",
    "@nx/node": "^21.4.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/aws-lambda": "^8.10.152",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/joi": "^17.2.2",
    "@types/node": "18.16.9",
    "esbuild": "^0.25.9",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "jest": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-util": "^30.0.2",
    "jsonc-eslint-parser": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "nx": "21.4.0",
    "prettier": "^2.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.29.0"
  },
